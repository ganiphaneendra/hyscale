name: frontend
image:
  registry: registry.hub.docker.com
  name: username/frontend # replace with your dockerhub username for {{username}}
  tag: 4.0.1

ports:
  - port: 8080/tcp
  - port: 5050/tcp

external: false

allowTraffic:
  - ports:
      - 8080/udp
      - 5050/http
    from:
      - web-service